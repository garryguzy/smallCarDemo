{
  "name": "webpack-starter",
  "version": "1.0.0",
  "description": "A light foundation for your next frontend project based on webpack.",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --config webpack/webpack.config.dev.js  --colors -w",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js  --colors",
    "start": "webpack-dev-server --open --config webpack/webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wbkd/webpack-starter.git"
  },
  "keywords": [
    "webpack",
    "startkit",
    "frontend",
    "es6",
    "javascript",
    "webdev"
  ],
  "author": "webkid.io",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wbkd/webpack-starter/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "7.7.4",
    "@typescript-eslint/eslint-plugin": "2.9.1-alpha.0",
    "@typescript-eslint/parser": "2.9.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.5",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "eslint": "6.7.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "2.18.2",
    "file-loader": "^4.2.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.1",
    "raw-loader": "4.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "ts-loader": "6.2.1",
    "typescript": "3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@types/greensock": "1.15.32",
    "@types/jquery": "3.3.31",
    "@types/rx": "4.1.1",
    "core-js": "^3.4.1",
    "leancloud-storage": "4.0.0",
    "mustache-loader": "1.4.3",
    "rxjs": "6.5.3",
    "scrollmagic": "2.0.7",
    "text-loader": "0.0.1"
  },
  "bit": {
    "env": {},
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
}
